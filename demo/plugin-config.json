{
    "sitesPatterns": [
        ".+://.*\\.superjob\\.ru.*",
        ".+://superjob\\.ru.*",
        ".+://www\\.avito\\.ru.*",
        ".+://avito\\.ru.*",
        ".+://joblab\\.ru.*",
        ".+://www\\.zarplata\\.ru.*",
        ".+://hr\\.zarplata\\.ru.*",
        ".+://hh\\.ru.*",
        ".+://.*\\.hh\\.ru.*"
    ],
    "pagesPatterns": [
        ".+://.*\\.superjob\\.ru/resume/[^\\?]+\\d+\\.html",
        ".+://www\\.avito\\.ru/rezume/[^\\?]+\\d+",
        ".+://avito\\.ru/rezume/[^\\?]+\\d+",
        ".+://www\\.avito\\.ru/.+/rezume/[^\\?]+\\d+",
        ".+://avito\\.ru/.+/rezume/[^\\?]+\\d+",
        ".+://joblab\\.ru/res\\d+\\.html",
        ".+://hr\\.zarplata\\.ru.*",
        ".+://hh\\.ru/resume/.*",
        ".+://.*\\.hh\\.ru/resume/.*"
    ],
    "config": [
        {
            "pattern": "superjob",
            "type": "SuperJob",
            "config": {
                "name": [
                    {
                        "selector": ".ResumeMainHR_content_name"
                    },
                    {
                        "selector": ".ResumeJustBought .sj_typo_medium"
                    },
                    {
                        "selector": ".ResumeMain_name"
                    },
                    {
                        "selector": ".ResumeMainHRNew .ResumeMainHRNew_content .sj_h3:not(.ng-hide)"
                    }
                ],
                "cell": [
                    {
                        "selector": ".ResumeMainHR_content_row.m_phone",
                        "unnecessaryPart": "\\s\\d{2}:\\d{2}—\\d{2}:\\d{2}"
                    },
                    {
                        "selector": ".ResumeJustBought .sj_typo_medium",
                        "index": 1,
                        "unnecessaryPart": "\\s\\d{2}:\\d{2}—\\d{2}:\\d{2}"
                    },
                    {
                        "selector": ".ResumeMain_contacts_item div",
                        "unnecessaryPart": "\\s\\d{2}:\\d{2}—\\d{2}:\\d{2}"
                    },
                    {
                        "selector": ".ResumeMainHRNew .ResumeMainHRNew_content .sj_block.sj_h3:not(.ng-hide)",
                        "unnecessaryPart": "\\s\\d{2}:\\d{2}—\\d{2}:\\d{2}"
                    },
                    {
                        "selector": ".sj-contact_phone .ng-binding",
                        "unnecessaryPart": "\\s\\d{2}:\\d{2}—\\d{2}:\\d{2}"
                    }
                ],
                "email": [
                    {
                        "selector": ".ResumeMainHR_content_row.m_email"
                    },
                    {
                        "selector": ".ResumeJustBought a"
                    },
                    {
                        "selector": ".ResumeMain_contacts_item div",
                        "index": 1
                    },
                    {
                        "selector": ".ResumeMainHRNew .ResumeMainHRNew_content .sj_block.sj_h3",
                        "index": 1,
                        "patternMatch": "^.+@.+$"
                    },
                    {
                        "selector": ".sj-contact_email .ng-binding",
                        "patternMatch": "^.+@.+$"
                    }
                ],
                "resume": [
                    ".ResumeMainHRNew_content",
                    ".ResumeDetailsNew_row",
                    ".ResumeMain_position",
                    ".ResumeMain_location",
                    ".ResumeMainHR_payment",
                    ".ResumeMainHR_content_row:not(.m_phone):not(.m_email)",
                    ".ResumeDetailsHR_line",
                    ".ResumeMain_block_padding > .ResumeMain_block_inside"
                ],
                "buyContacts": [
                    ".ResumeOfferBuyContacts",
                    ".ResumeBuyContactsForm_btn",
                    ".ResumeBuyContactsByPrice",
                    ".ResumeContactsToggle_toggle",
                    ".ResumeMainHR_data .sj_btn.m_green",
                    ".ResumeAddToFavoritesAndShowContacts"
                ]
            }
        },
        {
            "pattern": "avito",
            "type": "Avito",
            "config": {
                "name": [
                    {
                        "selector": ".seller-info-name"
                    }
                ],
                "cell": [
                    {
                        "selector": ".js-item-phone-button_card",
                        "patternMatch": "^[\\s\\d()\\+-]+$"
                    }
                ],
                "cellImage": [
                    {
                        "action": "getNodeAttribute",
                        "selector": ".js-item-phone-number img",
                        "attribute": "src"
                    }
                ],
                "resume": [
                    ".title-info-main",
                    ".item-price-value-wrapper",
                    ".item-params-list",
                    ".item-description",
                    ".resume-params"
                ],
                "buyContacts": [
                    ".item-phone-button_hide-phone"
                ]
            }
        },
        {
            "pattern": "joblab",
            "type": "JobMo",
            "config": {
                "name": [
                    {
                        "selector": ".table-to-div b",
                        "index": 0
                    }
                ],
                "cell": [
                    [
                        {
                            "action": "actionsUnion",
                            "actions": [
                                {
                                    "action": "waitMutation",
                                    "selector": "#p"
                                },
                                {
                                    "action": "click",
                                    "selector": "#p script + a"
                                }
                            ]
                        },
                        {
                            "selector": "#p",
                            "patternMatch": "^[\\s\\d(),\\+]+$"
                        }
                    ],
                    {
                        "selector": "#p",
                        "patternMatch": "^[\\s\\d(),\\+]+$"
                    },
                    {
                        "selector": ".table-to-div p",
                        "index": 3,
                        "patternMatch": "^[\\s\\d(),\\+]+$"
                    }
                ],
                "email": [
                    [
                        {
                            "action": "actionsUnion",
                            "actions": [
                                {
                                    "action": "waitMutation",
                                    "selector": "#m"
                                },
                                {
                                    "action": "click",
                                    "selector": "#m a:not([href*='mailto'])"
                                }
                            ]
                        },
                        {
                            "selector": "#m",
                            "patternMatch": "^[^@]+@[^@]+$"
                        }
                    ],
                    {
                        "selector": "#m",
                        "patternMatch": "^[^@]+@[^@]+$"
                    },
                    {
                        "selector": ".table-to-div p",
                        "index": 5,
                        "patternMatch": "^[^@]+@[^@]+$"
                    },
                    {
                        "selector": ".table-to-div p",
                        "index": 3,
                        "patternMatch": "^[^@]+@[^@]+$"
                    }
                ],
                "resume": [
                    ".table-to-div tbody tr:nth-child(n+6)"
                ],
                "buyContacts": [
                    "a[href='/employers/registr.php']"
                ]
            }
        },
        {
            "pattern": "zarplata.ru",
            "type": "Zarplata",
            "config": {
                "prepare": [
                    [
                        {
                            "action": "click",
                            "selector": ".pointer .down",
                            "all": true
                        },
                        {
                            "action": "next"
                        }
                    ],
                    {
                        "action": "actionsUnion",
                        "actions": [
                            {
                                "action": "waitMutation",
                                "selector": ".ui.container div.content",
                                "count": 3
                            },
                            {
                                "action": "click",
                                "selector": "div.content > div > a.ui.text.small"
                            }
                        ]
                    }
                ],
                "name": [
                    {
                        "selector": "div.content .big"
                    }
                ],
                "email": [
                    {
                        "selector": "div.content span.ui.text.small",
                        "index": 1,
                        "patternMatch": "^[^@]+@[^@]+$"
                    }
                ],
                "cell": [
                    {
                        "selector": "div.content span.ui.text.small",
                        "index": 0,
                        "patternMatch": "^[\\s\\d(),\\-\\+]+$"
                    }
                ],
                "resume": [
                    ".twelve > .padded > .row:nth-child(2)"
                ],
                "buyContacts": [
                    "div.content > div:last-child > :not(a)"
                ]
            }
        },
        {
            "pattern": "hh.ru",
            "type": "hh"
        }
    ]
}
