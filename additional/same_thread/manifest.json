{
  "manifest_version": 2,
  "name": "jsunderhood example",
  "version": "4.2",
  "icons": {
    "128": "./icons/icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "activeTab", "<all_urls>"],
  "browser_action": {
    "default_icon": { "128": "./icons/icon128.png" },
    "default_popup": "./popup/index.html"
  }
}
